  INCLUDES=-I../../lib
  DEPLIBS = $(DEPXAUTHLIB)
  LOCAL_LIBRARIES = ../../exports/lib/libXau.a
           SRCS = xauth.c gethost.c process.c parsedpy.c
           OBJS = xauth.o gethost.o process.o parsedpy.o
